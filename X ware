local Library = loadstring(game:HttpGet("https://pastebin.com/raw/vff1bQ9F"))()

local Window = Library.CreateLib("B Genesis V2", "BloodTheme")

local Tab = Window:NewTab("Blox Fruits")
local Section = Tab:NewSection("Farming (9 Scripts)")

Section:NewButton("Zaque Hub", "Op", function()
    loadstring(game:HttpGet("https://scriptblox.com/raw/UPDATE-16-Blox-Fruits-ZaqueHub-9508"))()
end)

Section:NewButton("Domadic Hub", "Domadic hub", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Domadicoof/Domadicoof/main/Domadichub/NottoGay/Start.ranscript"))()
end)

Section:NewButton("Sonic Hub", "Sonic Hub", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/NickelHUBB/SonicTuru/main/Protected-25.lua"))()
end)

Section:NewButton("Pado Hub", "Really good", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/REWzaKunGz1/premium/main/PadoHub'))()
end)

Section:NewButton("Hoho Hub", "top 2", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/acsu123/HOHO_H/main/Loading_UI'))()
end)

Section:NewButton("Zen Hub", "", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Kaizenofficiall/ZenHub/main/Loader", true))()
end)

Section:NewButton("Uranium Hub", "Top Tier", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Augustzyzx/UraniumMobile/main/UraniumKak.lua"))()
end)

Section:NewButton("Neva hub", "Some bugs lol", function()
    loadstring(game:HttpGet("https://scriptblox.com/raw/UPDATE-16-Blox-Fruits-Neva-Hub-9912"))()
end)

Section:NewButton("Blck Hub", "Keyless", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/1f0yt/community/main/BLCK"))()
end)

local ManualSection = Tab:NewSection("Manual (1 Script)")

ManualSection:NewButton("Sword Dealer+Hop", "Finding Server with sword dealer", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/vinhuchi/rblx/main/W-azure/Free/AutoSwordHop.lua"))()
end)

local ArsenalTab = Window:NewTab("Arsenal")
local SecondTabSection = ArsenalTab:NewSection("OP Scripts (2 Scripts)")

SecondTabSection:NewButton("Zen Hub", "", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/Kaizenofficiall/ZenHub/main/Loader"))()
end)

SecondTabSection:NewButton("Godmode", "Never die!", function()
    loadstring(game:HttpGet("https://scriptblox.com/raw/Arsenal-Godmode-4604"))()
end)

SecondTabSection:NewToggle("Bypass Anticheat", "Beta", function(state)
    if state then
        -- AC bypass enabled
        game.StarterGui:SetCore("SendNotification", {
            Title = "Anticheat Disabled",
            Text = "May not work on most executors!",
            Duration = 3
        })
    else
        -- AC bypass disabled
        game.StarterGui:SetCore("SendNotification", {
            Title = "ERROR",
            Text = "Rejoin to Unbypass",
            Duration = 10
        })
    end
end)

-- New "Universal" Tab
local UniversalTab = Window:NewTab("Universal")
local UniversalSection = UniversalTab:NewSection("Works at any Game (2 Scripts)")

-- Button 1: Infinite Yield
UniversalSection:NewButton("Infinite Yield", "45% VISUAL", function()
    loadstring(game:HttpGet("https://scriptblox.com/raw/Infinite-Yield_500"))()
end)

-- Button 2: Fly Gui v3
UniversalSection:NewButton("Fly Gui v3", "Fly for Mobile!", function()
    loadstring(game:HttpGet("https://scriptblox.com/raw/Universal-Script-Fly-GUI-V3-10610"))()
end)

Window:Ready()
Window:MoveTo(game:GetService("CoreGui"))
